# ===== Solana / PNP =====
# Use devnet for hackathon
RPC_URL=https://api.devnet.solana.com

# Wallet private key for signing (base58 OR JSON array string).
# Supports: PNP_PRIVATE_KEY, DEVNET_PRIVATE_KEY, or TEST_PRIVATE_KEY
PNP_PRIVATE_KEY=
# DEVNET_PRIVATE_KEY=  # Alternative name (also works)
# TEST_PRIVATE_KEY=     # Alternative name (also works)

# Optional: custom oracle pubkey (defaults to wallet public key)
ORACLE_ADDRESS=

# Collateral mint (YOUR custom token that you already minted! it is custom spl-token on devnet deployed by us)
COLLATERAL_MINT=CynN8io5GiG4RGutAscXYKLdx56zJsP3dL8fmy4EYpmW
COLLATERAL_DECIMALS=6
NEXT_PUBLIC_COLLATERAL_DECIMALS=6
NEXT_PUBLIC_COLLATERAL_LABEL=USDC
NEXT_PUBLIC_COLLATERAL_MINT=CynN8io5GiG4RGutAscXYKLdx56zJsP3dL8fmy4EYpmW

# ===== Market params =====
INITIAL_LIQUIDITY_BASE_UNITS=1000000
SEED_TRADE_USDC=1
YES_ODDS_BPS=5000
MARKET_DURATION_SECONDS=120

# ===== Mint your own collateral (recommended) =====
TOKEN_DECIMALS=6
MINT_AMOUNT_UI=10000000

# ===== Event source (privacy) =====
# Option A: GitHub private repo signals (requires token with repo read)
GITHUB_TOKEN=
GITHUB_OWNER=
GITHUB_REPO=

# A tiny "private API" mode (no network). Provide a local JSON file path.
PRIVATE_EVENT_JSON_PATH=./data/private-events.json

# ===== LLM oracle =====
# Using Gemini (OpenAI compatible endpoint)
LLM_BASE_URL=https://generativelanguage.googleapis.com/v1beta/openai/
LLM_API_KEY=
LLM_MODEL=gemini-2.5-flash

# ===== Discord Bot Integration =====
# Get bot token from https://discord.com/developers/applications
DISCORD_BOT_TOKEN=
# Right-click channel → Copy Channel ID (enable Developer Mode in Discord settings)
DISCORD_CHANNEL_ID=
# Right-click server → Copy Server ID
DISCORD_GUILD_ID=
# Command prefix for predictions (default: !predict)
DISCORD_TRIGGER_PREFIX=!predict